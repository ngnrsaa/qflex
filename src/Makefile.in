TARGET1 = qflex

OBJS1 = circuit.o grid.o evaluate_circuit.o tensor.o contraction_utils.o read_circuit.o docopt/docopt.o

CXXFLAGS += -Idocopt/

$(TARGET1): main.cpp $(OBJS1)
	$(CXX) -o $(@).x $< $(OBJS1) $(CXXFLAGS)

%.o: %.cpp %.h
	$(CXX) -c $< $(CXXFLAGS) -o $@

.PHONY: clean
clean:
	-rm -f ./*.x ./*.a ./*.so ./*.mod $(OBJS1)
